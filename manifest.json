{
	"manifest_version": 2,
	"name": "Mocking Spongebob Selection Shortcut",
	"version": "1.0",
	"description": "Generate Mocking Spongebob memes faster with a context menu shortcut!",
	"permissions": [
		"contextMenus",
		"tabs",
		"activeTab",
		"https://denk0403.github.io/Mocking-Spongebob/*"
	],
	"background": {
		"scripts": ["./src/background.js"],
		"persistent": false
	},
	"icons": {
		"144": "img/icon-144.png"
	}
}
